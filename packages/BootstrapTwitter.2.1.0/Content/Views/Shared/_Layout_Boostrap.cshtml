   <link href="@Url.Content("~/Scripts/google-code-prettify/prettify.css")" rel="stylesheet" type="text/css" />
   <link rel="shortcut icon" type="image/x-icon" href="http://twitter.github.com/bootstrap/assets/ico/favicon.ico">
</head>
<body>
